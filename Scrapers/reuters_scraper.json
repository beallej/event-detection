{
	"id" : "REUTERS",
    "class" : "eventdetection.downloader.PythonScraper",
    "python" : {
    	"scripts" : {
    		"sectioning" : ["../ReutersScraper.py"]
    	},
    	"parameters" : {
    		"sectioning" : {},
			"textId" : "articleText"
    	}
    },
	"sectioning" : [["<span id=\"articleText\">(.*?)</span>", "$1\n"]],
	"filtering" : [["<p>.*?</p>", "$1"], ["<a.*?>(.*?)</a>", "$1"]]
}
